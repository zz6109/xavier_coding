# Insert the string below into the .bashrc 

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/kimjaehwan/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/kimjaehwan/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/home/kimjaehwan/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/kimjaehwan/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

source /opt/ros/noetic/setup.bash
